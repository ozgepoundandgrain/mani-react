{
  "expo": {
    "name": "manifestation-react",
    "description": "Create your vision board and journaling for manifesting.",
    "slug": "manifestation-react",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.0",
